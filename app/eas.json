{
  "cli": {
    "version": ">= 14.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "autoIncrement": true
      }
    },
    "test": {
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "test"
      },
      "android": {
        "buildType": "apk",
        "autoIncrement": true
      }
    },
    "production": {
      "android": {
        "buildType": "apk",
        "autoIncrement": true
      }
    }
  },
  "submit": {
    "production": {}
  }
}
